!! cursor
Xcursor.theme: Adwaita

!! font settings
Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!! xterm
XTerm*termName: xterm-256color
XTerm*faceName: Ubuntu Mono
XTerm*faceSize: 11
XTerm*renderFont: true
XTerm.vt100.metaSendsEscape: true
XTerm.vt100.savelines: 4096
XTerm.vt100.visualBell: false
XTerm*vt100.Translations: #override \
                 Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
                 Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)

XTerm.vt100.foreground: #303030
XTerm.vt100.background: #FFFFFF

!black
*color0:#303030
!*color0:#242424
*color8:#4B5050
!red
*color1:#E47272
*color9:#FE7F7F
!green
*color2:#5CB899
*color10:#66CCAA
!yellow
*color3:#E3D071
*color11:#FFEA80
!blue
*color4:#73ACE5
*color12:#80BFFF
!magenta
*color5:#6666CC
*color13:#8080FF
!cyan
*color6:#4B9696
*color14:#5CB8B8
!white
*color7:#E3E3E3
*color15:#FFFFFF
